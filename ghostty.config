theme=ayu
cursor-style = block
window-height=24
window-width=80
window-padding-x = 8
window-padding-y = 0, 4
window-colorspace = "display-p3"
window-save-state = "never"
shell-integration-features = no-cursor
cursor-color = #646464
title = " "
macos-titlebar-proxy-icon="hidden"
font-size = 12.5
unfocused-split-opacity = 0.7
term = xterm-256color
keybind = global:ctrl+shift+t=new_window
